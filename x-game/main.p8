version 42
__lua__
function _init()
	init_map()
end

function _update()
	if not p.m.dir then
		if btn(⬅️) then start_move(p, ⬅️) end
		if btn(➡️) then start_move(p, ⬅️) end
		if btn(⬆️) then start_move(p, ⬅️) end
		if btn(⬇️) then start_move(p, ⬅️) end
	end
end

function _draw()
	cls()
	draw_walls()
	spr(1,p.x,p.y)
end
-->8
--movement

function start_move(o, dir)
	local m_x = o.x/8
	local m_y = o.y/8
	if dir == ⬅️ then
		m_x = max(0, t_x-1) end
	if dir == ➡️ then
		m_x = min(15, t_x+1) end
	if dir == ⬆️ then
		m_y = max(0, t_y-1) end
	if dir == ⬇️ then
		m_y = min(15, t_y+1) end
	local m = mget(t_x, t_y)
	
end


-->8
--map
function init_map()
	walls={}
	p={x=-1,y=-1,m={}}
	for i=0,15 do for j=0,15 do
		local m = mget(i,j)
		if m == 1 then
			p.x=i*8 p.y=j*8
		end
		if m == 2 then
			add(walls, {x=i*8, y=j*8})
		end
	end end
end

function draw_walls()
	for w in all(walls) do
		spr(2,w.x,w.y)
	end
end
